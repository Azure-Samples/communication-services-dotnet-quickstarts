{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    //connection string of communication resource
    "ResourceConnectionString": "%ResourceConnectionString%",
    //web pubsub URI
    "MediaStreamingTransportURI": "%MediaStreamingTransportURI%",
    //url of the deployed API
    "AppCallBackUri": "%AppCallBackUri%",
    //Query string for callback URL
    "SecretValue": "%SecretValue%",
    // Ngrok Exe Path
    "NgrokPath": "%NgrokExePath%",
    //Accept Calls From specific participants only
    //or "*" for accepting calls from all participants
    "AcceptCallsFrom": "*"
}
