{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  //connection string of communication resource
  "ResourceConnectionString": "%ResourceConnectionString%",
  //url of the deployed API
  "AppCallBackUri": "%AppCallBackUri%",
  //public url of wav audio
  "AudioFileUri": "%AudioFileUri%",
  //participant (PhoneNumber/MRI)
  "TargetParticipant": "%Targetpartcipant%",
  //Query string for callback URL
  "SecretValue": "%SecretValue%",
  //IVR participants (ACS resource IDs separated by ',' or "*" for accepting all incoming calls)
  //For e.g. 8:acs:ab12b0ea-85ea-4f83-b0b6-84d90209c7c4_00000009-bce0-da09-54b7-xxxxxxxxxxxx)
  // eg 8:acs:ab12b0ea-85ea-4f83-b0b6-84d90209c7c4_00000009-bce0-da09-54b7-xxxxxxxxxxxx , 8:acs:ab12b0ea-85ea-4f83-b0b6-84d90209c7c4_00000009-bce0-da09-54b7-xxxxxxxxxxxx
  "IVRParticipants": "*"
}
